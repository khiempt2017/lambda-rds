{
  "name": "bp-api-serverless",
  "version": "1.0.0",
  "description": "The lambda function build for Typescript",
  "author": "vfa-KhiemPT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start:dev": "concurrently \"tsc -w\" \"sam local start-api --docker-network local-network --skip-pull-image --env-vars local.env.json\"",
    "start:test": "concurrently \"tsc\" \"sam local start-api --docker-network local-network --warm-containers EAGER --skip-pull-image --env-vars local.env.json\"",
    "lint:fix": "eslint --fix",
    "test": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --coverage",
    "test:watch": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --watchAll",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "aws-sdk-client-mock": "^4.0.2",
    "esbuild-jest": "^0.4.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-pdf-node": "^1.0.7",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47"
  }
}
